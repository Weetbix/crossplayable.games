{
  "name": "crossplayable.games",
  "private": true,
  "description": "A website for for showing crossplay abiliy of various games",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_CONCURRENT_DOWNLOAD=6 gatsby build",
    "develop": "GATSBY_CONCURRENT_DOWNLOAD=6 gatsby develop",
    "develop:debug": "GATSBY_CONCURRENT_DOWNLOAD=6 node --nolazy node_modules/.bin/gatsby develop --inspect-brk",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cypress:open",
    "cypress:open": "cypress open",
    "deploy:s3": "aws s3 sync public s3://crossplayable.games"
  },
  "dependencies": {
    "@types/color": "^3.0.1",
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "^2.32.6",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-advanced-sitemap": "^1.5.6",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-layout": "^1.3.13",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-plausible": "^0.0.7",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sharp": "^2.14.3",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-source-filesystem": "^2.3.34",
    "gatsby-transformer-sharp": "^2.5.17",
    "react": "^17.0.1",
    "react-circular-progressbar": "^2.0.3",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "styled-breakpoints": "^9.0.9",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.4",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.4",
    "cypress": "^6.5.0",
    "jsdom": "^16.4.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "prettier": "2.2.1",
    "start-server-and-test": "^1.12.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Weetbix/crossplayable.games"
  },
  "bugs": {
    "url": "https://github.com/Weetbix/crossplayable.games/issues"
  }
}
