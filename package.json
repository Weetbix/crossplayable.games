{
  "name": "crossplayable.games",
  "private": true,
  "description": "A website for for showing crossplay abiliy of various games",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_CONCURRENT_DOWNLOAD=6 gatsby build",
    "develop": "GATSBY_CONCURRENT_DOWNLOAD=6 gatsby develop",
    "develop:debug": "GATSBY_CONCURRENT_DOWNLOAD=6 node --nolazy node_modules/.bin/gatsby develop --inspect-brk",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:unit": "jest",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cypress:open",
    "cypress:open": "cypress open",
    "deploy:s3": "aws s3 sync public s3://crossplayable.games"
  },
  "dependencies": {
    "@types/color": "^3.0.3",
    "babel-plugin-styled-components": "^2.0.7",
    "gatsby": "^4.7.1",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-advanced-sitemap": "^2.1.0",
    "gatsby-plugin-graphql-codegen": "^3.1.1",
    "gatsby-plugin-layout": "^3.12.1",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-plausible": "^0.0.7",
    "gatsby-plugin-react-helmet": "^5.12.1",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sharp": "^4.13.0",
    "gatsby-plugin-styled-components": "^5.12.1",
    "gatsby-source-filesystem": "^4.12.1",
    "gatsby-transformer-sharp": "^4.15.0",
    "react": "^17.0.2",
    "react-circular-progressbar": "^2.0.4",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "styled-breakpoints": "^11.0.5",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/jsdom": "^16.2.14",
    "@types/react-helmet": "^6.1.5",
    "@types/styled-components": "^5.1.25",
    "babel-jest": "^28.0.3",
    "cypress": "^9.6.0",
    "jest": "^28.0.3",
    "jsdom": "^19.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "prettier": "2.6.2",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Weetbix/crossplayable.games"
  },
  "bugs": {
    "url": "https://github.com/Weetbix/crossplayable.games/issues"
  },
  "prettier": {
    "singleQuote": false,
    "semi": true
  }
}
